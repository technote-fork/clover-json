{
  "name": "@technote-space/clover-json",
  "version": "0.4.30",
  "description": "Parse clover XML coverage reports to JSON, using the same format as lcov-parse",
  "author": "TobiLG",
  "contributors": [
    "Technote <technote.space@gmail.com> (https://technote.space)"
  ],
  "license": "MIT",
  "keywords": [
    "coverage",
    "report",
    "clover",
    "xml",
    "cover",
    "json",
    "lcov",
    "parse",
    "parser",
    "converter"
  ],
  "homepage": "https://github.com/technote-space/clover-json",
  "repository": {
    "type": "git",
    "url": "https://github.com/technote-space/clover-json.git"
  },
  "bugs": {
    "url": "https://github.com/technote-space/clover-json/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/jest": "^26.0.13",
    "@types/node": "^14.6.4",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "eslint": "^7.8.1",
    "jest": "^26.4.2",
    "jest-circus": "^26.4.2",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "test": "yarn lint && yarn cover",
    "lint": "eslint 'src/**/*.ts' '__tests__/**/*.ts' --cache",
